{
  "name": "audius",
  "version": "2.0.17",
  "author": "Falko Krause",
  "description": "Play media files posted in conversations",
  "main": "index.js",
  "scripts": {
    "start": "node ./config/webpack.devserver.js",
    "test": "ssh -t rockdapus 'rm /var/www/audius/*.gz'",
    "build:prod:app": "./node_modules/.bin/webpack --config='./config/webpack.production.app.js' --progress --display-error-details",
    "build:prod:single-file-app": "./node_modules/.bin/webpack --config='./config/webpack.production.single-file-app.js' --progress --display-error-details && ./src/scripts/single-file-release.js",
    "build": "./node_modules/.bin/webpack --config='./config/webpack.base.js' --progress --display-error-details",
    "build:extension": "./node_modules/.bin/webpack --config='./config/webpack.dev.extension.js' --progress --display-error-details",
    "docu": "livingstyleguide compile ./src/sass/styleguide.lsg ./documentation/styleguide.html && nodejs node_modules/.bin/generate-md --layout github --input README.md --output ./documentation/",
    "extension:rls": "npm run version; npm run build:prod:extension; cd dist-extension/; zip -r ../audius.extension.zip . *",
    "version": "src/scripts/bump-version.js",
    "build:prod:extension": "./node_modules/.bin/webpack --config='./config/webpack.production.extension.js' --progress --display-error-details",
    "icons": "./config/convert-icomoon.js ./src/icons/media-player.zip ./src/sass/_icon.map.sass ./src/fonts",
    "serve-static": "http-server -p 8080 ./dist-website",
    "deploy": "npm run build:prod:single-file-app && npm run build:prod:app && ssh -t rockdapus 'rm /var/www/audius/*.gz' || echo 'no gz' && scp -r dist-website/* rockdapus:/var/www/audius/"
  },
  "license": "gnu",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "fuse.js": "^3.2.1",
    "matrix-js-sdk": "^0.11.0",
    "olm": "https://matrix.org/packages/npm/olm/olm-2.3.0.tgz",
    "snarkdown": "^1.2.2",
    "sortablejs": "^1.10.2",
    "vue": "^2.6.11",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.4.0",
    "webextension-polyfill": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "adm-zip": "^0.4.14",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "circular-dependency-plugin": "^5.2.0",
    "compression-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^6.7.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "http-server": "^0.12.3",
    "markdown-styles": "^3.1.10",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.14.1",
    "postcss-loader": "^7.0.2",
    "raw-loader": "^4.0.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.2.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0"
  }
}